				//---------------------------
				.section
					.content
						//изображения
						.wrap-image.clearfix
							figure.box-image_left
								img.box_img(alt="Контент" src="images/wrap-image.png")
							figure.box-image_right
								img.box_img(alt="Контент" src="images/wrap-image.png")
				//----------------------------
				.section
					.content
						//команда
						.wrap-team
							//заголовок
							h2.team-title OUR TEAM
							//строка профилей х5
							.team-row
								//элемент профиля
								//------------------------------------							
								mixin team-row_item(teamName, teamPost)
									.team-row_item
										figure.box-team
											img.box_img(alt="Команда" src="images/team.png")
											figcaption.team_name= teamName
												p.team_post= teamPost
								//-------------------------------------
								+team-row_item ("John Doe", "ceo")
								+team-row_item ("Saundra Pittsley", "team leader")
								+team-row_item ("Julio Simser", "senior developer")
								+team-row_item ("Margery Venuti", "senior developer")
								+team-row_item ("Fernando Tondrea", "developer")
							//строка профилей х5
							.team-row
								//элемент профиля
								+team-row_item ("Ericka Nobriga", "art director")
								+team-row_item ("Cody Rousselle", "senior ui designer")
								+team-row_item ("Erik Wollman", "senior ui designer")
								+team-row_item ("Dona Shoff", "ux designer")
								+team-row_item ("Darryl Brunton", "ui designer")
			//wrapper end
			
			//sidebar
			aside.sidebar
				//nav
				.menu
					ul.side-menu-list
						//--------------------------
						- var nav_button = ["LOREM IPSUM", "DONEC TINCIDUNT LAOREET", "VESTIBULUM ELIT", "ETIAM PHARETRA", "PHASELLUS PLACERAT", "CRAS ET NISI VITAE ODIO"];
						for name_button in nav_button
							//----------------------
							li.side-menu-item
								a.menu-link(href='#')= name_button
				//photo
				.side-box_inner
					.side-photo
						h2.photo_title.
							OUR OFFICES
						figure.photo_image
							img.photo_img(alt="Фото офиса" src="images/side-photo.png")
			//sidebar end
		
		//footer
		footer.footer.clearfix
			.footer-inner.clearfix
				.footer-twitter
					.twitter
						h3.footer_title.
							TWITTER FEED
						time.twitter_time(datetime="2017-07-16")
							a(href="#").
								23 oct
						p.twitter-text.
							In ultricies pellentesque massa a porta. Aliquam ipsum enim, hendrerit ut porta nec, ullamcorper et nulla. 
							In eget mi dui, sit amet scelerisque nunc. Aenean aug
				.footer-sitemap
					.sitemap
						h3.footer_title.
							SITEMAP
						.sitemap_links
							a(href='#').
								Home
							a(href='#').
								About
							a(href='#').
								Services
						.sitemap_links.sitemap_links-right
							a(href='#').
								Partners
							a(href='#').
								Support
							a(href='#').
								Contact
				.footer-social
					.social
						h3.footer_title.
							SOCIAL NETWORKS
						a.social-icon.twitter(href="#") 
						a.social-icon.facebook(href="#") 
						a.social-icon.google-plus(href="#") 
						a.social-icon.social-icon-small.vimeo(href="#") 
						a.social-icon.social-icon-small.youtube(href="#") 
						a.social-icon.social-icon-small.flickr(href="#")
						a.social-icon.social-icon-small.instagram(href="#")
						a.social-icon.social-icon-small.rss(href="#")
				.footer-logo
					.f_logo-box
						.f_logo-image_box
							.f_logo-image
								a(href='#')
									img.f_logo_img(alt="Логотип" src="images/footer-logo.png")
						.f_logo-image_text
							p.
								Copyright © 2012 Whitesquare. A #[a(href="#") pcklab] creation
							
							
							
							
